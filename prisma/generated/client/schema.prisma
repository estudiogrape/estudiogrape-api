generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Contato {
  id         String   @id @default(uuid())
  nome       String
  email      String   
  phone      String   
  created_at DateTime @default(now())

  @@map("grape_contato")
}


model GoodSun_Email {
  id         String   @id @default(uuid())
  email      String   
  created_at DateTime @default(now())

  @@map("goodsun_email")
}